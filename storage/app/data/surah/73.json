                "surah": 73,
                "page": 574,
                "manzil": 508,
                "hizbQuarter": 229,
                "sajda": {
                    "recommended": false,
                    "obligatory": false
                }
            },
            "text": {
                "arab": "\u0648\u064e\u0627\u0630\u0652\u0643\u064f\u0631\u0650 \u0627\u0633\u0652\u0645\u064e \u0631\u064e\u0628\u0651\u0650\u0643\u064e \u0648\u064e\u062a\u064e\u0628\u064e\u062a\u0651\u064e\u0644\u0652 \u0627\u0650\u0644\u064e\u064a\u0652\u0647\u0650 \u062a\u064e\u0628\u0652\u062a\u0650\u064a\u0652\u0644\u064b\u0627\u06d7",
                "transliteration": {
                    "en": "Wazkuris ma rabbika wa tabattal ilaihi tabteelaa",
                    "id": "wa\u017ckurisma rabbika wa tabattal ilaihi tabt\u012bl\u0101(n)."
                }
            },
            "translation": {
                "en": "And remember the name of your Lord and devote yourself to Him with [complete] devotion.",
                "id": "Dan sebutlah nama Tuhanmu, dan beribadahlah kepada-Nya dengan sepenuh hati."
            },
            "audio": {
                "primary": "https:\/\/cdn.islamic.network\/quran\/audio\/128\/ar.alafasy\/5483.mp3",
                "secondary": [
                    "https:\/\/cdn.islamic.network\/quran\/audio\/64\/ar.alafasy\/5483.mp3"
                ]
            },
            "tafsir": {
